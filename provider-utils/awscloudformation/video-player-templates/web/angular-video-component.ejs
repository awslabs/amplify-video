<video-player 
    [options]="{
        autoplay: true, 
        controls: true,
<% if (channelLatency === 'LOW') { -%>
        src: '<%= src %>',
        techOrder:['AmazonIVS']}">
<% } else { -%>
        sources: [{ 
            src: '<%= src %>', 
            type: 'application/x-mpegURL' 
        }]}">
<% } -%>
</video-player>