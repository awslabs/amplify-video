{
    "resourceName": {
        "key": "resourceName",
        "question": "Provide a friendly name for your resource to be used as a label for this category in the project:",
        "validation": {
            "operator": "regex",
            "value": "^[a-zA-Z0-9\\-]+$",
            "onErrorMsg": "Resource name should be alphanumeric"
        },
        "required": true,
        "default": "myivs"
    },
    "cvsEnable": {
        "key": "cvsEnable",
        "question": "Do you want to create a single channel or an API to manage multiple channels?",
        "type": "list",
        "options": [
            {
                "name": "Single IVS Channel",
                "value": "single"
            },
            {
                "name": "Multiple IVS Channels",
                "value": "vms"
            }
        ],
        "default": "single"
    },
    "channelQuality": {
        "key": "channelQuality",
        "question": "Choose the default stream quality:",
        "type": "list",
        "options": [
            {
                "name": "Standard (HD Stream)",
                "value": "STANDARD"
            },
            {
                "name": "Basic (SD Stream)",
                "value": "BASIC"
            }
        ],
        "default": "STANDARD",
        "required": true
    },
    "channelLatency": {
        "key": "channelLatency",
        "question": "Choose the default stream latency type:",
        "type": "list",
        "options": [
            {
                "name": "Ultra-low latency (~5 seconds)",
                "value": "LOW"
            },
            {
                "name": "Standard latency (~10 seconds) ",
                "value": "NORMAL"
            }
        ],
        "default": "LOW",
        "required": true
    }
}
